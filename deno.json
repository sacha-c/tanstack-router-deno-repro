{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite"
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "singleQuote": true
    }
  }
}